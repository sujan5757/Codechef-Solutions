#include <stdio.h>

int main(void) {
    int t;
    scanf("%d", &t);
    while(t!=0)
    {
        int x,y;
        scanf("%d %d", &x, &y);
        if (x>=y)
        {
            printf("yes \n");
        }
        else
        printf("no \n");
        t--;
    }
	// your code goes here
	return 0;
}

